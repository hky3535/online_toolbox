[
    {
        "en_name": "Media Convert",
        "zh_name": "格式转换",
        "input_suffix_name": "mp3&wav&mp4&avi",
        "line": "ffmpeg -i {input} {output}.{suffix_name}",
        "params": {
            "suffix_name": {
                "en_name": "Suffix Name",
                "zh_name": "后缀名",
                "describe": "Suffix Name",
                "type": "str",
                "range": ["mp3", "wav", "mp4", "avi"],
                "default": "mp4"
            }
        }
    },

    {
        "en_name": "Video Compress",
        "zh_name": "视频压缩",
        "input_suffix_name": "mp4&avi",
        "line": "ffmpeg -i {input} -vcodec libx264 -crf {compress_rate} {output}.{suffix_name}",
        "params": {
            "compress_rate": {
                "en_name": "Compress Rate",
                "zh_name": "压缩率",
                "describe": "Compress Rate",
                "type": "int",
                "range": [0, 51],
                "default": 51
            },
            "suffix_name": {
                "en_name": "Suffix Name",
                "zh_name": "后缀名",
                "describe": "Suffix Name",
                "type": "str",
                "range": ["mp4", "avi"],
                "default": "mp4"
            }
        }
    },
    
    {
        "en_name": "Audio Compress",
        "zh_name": "音频压缩",
        "input_suffix_name": "mp3&wav",
        "line": "ffmpeg -i {input} -ar {sampling_rate} -b:a {bitrate}k -filter:a atempo={speed_rate},volume={volume_rate} {output}.{suffix_name}",
        "params": {
            "sampling_rate": {
                "en_name": "Sampling Rate",
                "zh_name": "采样率",
                "describe": "Sampling Rate",
                "type": "int",
                "range": [8000, 192000],
                "default": 51
            },
            "bitrate": {
                "en_name": "Bitrate",
                "zh_name": "比特率",
                "describe": "Bitrate",
                "type": "int",
                "range": [128, 8000],
                "default": "mp4"
            },
            "speed_rate": {
                "en_name": "Speed Rate",
                "zh_name": "速度",
                "describe": "Speed Rate",
                "type": "float",
                "range": [0.1, 10.0],
                "default": 1.0
            },
            "volume_rate": {
                "en_name": "Volume Rate",
                "zh_name": "音量",
                "describe": "Volume Rate",
                "type": "float",
                "range": [0.0, 10.0],
                "default": 1.0
            },
            "suffix_name": {
                "en_name": "Suffix Name",
                "zh_name": "后缀名",
                "describe": "Suffix Name",
                "type": "str",
                "range": ["mp3", "wav"],
                "default": "mp3"
            }
        }
    },

    {
        "en_name": "Media Cut",
        "zh_name": "媒体剪切",
        "input_suffix_name": "mp3&wav&mp4&avi",
        "line": "ffmpeg -i {input} -ss {start_time} -t \"{duration}\" -c copy {output}.{suffix_name}",
        "params": {
            "start_time": {
                "en_name": "Start Time",
                "zh_name": "起始时间",
                "describe": "Start Time",
                "type": "float",
                "range": [0, 999999],
                "default": "0"
            },
            "duration": {
                "en_name": "Duration",
                "zh_name": "时长",
                "describe": "Duration",
                "type": "float",
                "range": [0, 999999],
                "default": "10"
            },
            "suffix_name": {
                "en_name": "Suffix Name",
                "zh_name": "后缀名",
                "describe": "Suffix Name",
                "type": "str",
                "range": ["mp3", "wav", "mp4", "avi"],
                "default": "mp4"
            }
        }
    }
]